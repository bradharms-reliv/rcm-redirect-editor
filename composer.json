{
  "name": "rcm/redirect-editor",
  "description": "Rcm Plugin module to edit all parts of a redirect.",
  "type": "library",
  "license": "BSD-3-Clause",
  "keywords": [
    "rcm",
    "module",
    "zf2",
    "redirect"
  ],
  "minimum-stability": "stable",
  "homepage": "https://github.com/bjanish/rcm-redirect-editor",
  "authors": [
    {
      "name": "Brian Janish",
      "email": "bjanish@relivinc.com",
      "homepage": "https://github.com/reliv"
    }
  ],
  "require": {
    "php": ">=5.5",
    "zendframework/zendframework": "2.*",
    "fxp/composer-asset-plugin": "^1.2.0",
    "rcm/core": "^1.20.0",
    "rcm/core-js": "^1.0.0",
    "rcm/admin": ">=1.3.0",
    "bower-asset/bootstrap": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "RcmRedirectEditor\\": "src/"
    }
  },
  "extra": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-vcs-driver-options": {
      "github-no-api": true
    }
  },
  "config": {
    "asset-pattern-skip-version": "(-patch*)",
    "asset-installer-paths": {
      "bower-asset-library": "public/bower_components"
    }
  }
}
